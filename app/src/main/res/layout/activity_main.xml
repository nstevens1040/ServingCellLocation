<!--        tools:layout_editor_absoluteX="0dp"-->
<!--        app:layout_constraintBottom_toBottomOf="parent"-->
<!--        app:layout_constraintStart_toStartOf="parent"-->
<!--        app:layout_constraintEnd_toEndOf="parent"-->
<!--        tools:layout_editor_absoluteY="3dp"-->
<!--        android:layout_height="0dp"-->
<!--        />-->
    <TextView
        android:id="@+id/signal"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:includeFontPadding="false"
        android:singleLine="false"
        android:text=""
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/signal_label"
        app:layout_constraintTop_toBottomOf="@+id/coords_title"
        app:layout_constraintVertical_bias="0.0" />
    <TextView
        android:id="@+id/mcc"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:includeFontPadding="false"
        android:singleLine="false"
        android:text=""
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/mcc_label"
        app:layout_constraintTop_toBottomOf="@+id/signal"
        app:layout_constraintVertical_bias="0.0" />

    <!--
    app:layout_constraintBottom_toTopOf="@+id/mnc"
        android:layout_marginTop="-20dp"
    -->
    <!-- app:layout_constraintEnd_toEndOf="parent" -->
    <TextView
        android:id="@+id/mnc"
        android:layout_width="200dp"
        android:text=""
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@+id/mcc"
        tools:layout_editor_absoluteX="0dp"
        app:layout_constraintStart_toEndOf="@+id/mnc_label"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="3dp"
        android:includeFontPadding="false"
        android:layout_height="40dp"
        />
<!--    app:layout_constraintBottom_toTopOf="@+id/lac"-->
    <TextView
        android:id="@+id/lac"
        android:layout_width="200dp"
        android:text=""
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@+id/mnc"
        tools:layout_editor_absoluteX="0dp"
        app:layout_constraintStart_toEndOf="@+id/lac_label"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="3dp"
        android:includeFontPadding="false"
        android:layout_height="40dp"
        />
<!--    app:layout_constraintBottom_toTopOf="@+id/cid"-->
    <TextView
        android:id="@+id/cid"
        android:layout_width="200dp"
        android:text=""
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@+id/lac"
        tools:layout_editor_absoluteX="0dp"
        app:layout_constraintStart_toEndOf="@+id/cid_label"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="3dp"
        android:includeFontPadding="false"
        android:layout_height="40dp"
        />
    <TextView
        android:id="@+id/enb"
        android:layout_width="200dp"
        android:text=""
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@+id/cid"
