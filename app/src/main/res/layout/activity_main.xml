        app:layout_constraintStart_toEndOf="@+id/enb_label"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="3dp"
        android:includeFontPadding="false"
        android:layout_height="40dp"
        />
<!--    app:layout_constraintBottom_toTopOf="@+id/lat"-->
    <TextView
        android:id="@+id/lat"
        android:layout_width="200dp"
        android:text=""
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@+id/enb"
        tools:layout_editor_absoluteX="0dp"
        app:layout_constraintStart_toEndOf="@+id/lat_label"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="3dp"
        android:includeFontPadding="false"
        android:layout_height="40dp"
        />
<!--    app:layout_constraintBottom_toTopOf="@+id/lng"-->
    <TextView
        android:id="@+id/lng"
        android:layout_width="200dp"
        android:text=""
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@+id/lat"
        tools:layout_editor_absoluteX="0dp"
        app:layout_constraintStart_toEndOf="@+id/lng_label"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="3dp"
        android:includeFontPadding="false"
        android:layout_height="40dp"
        />
<!--    app:layout_constraintBottom_toBottomOf="parent"-->
<!--
labels
-->
    <TextView
        android:id="@+id/signal_label"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:includeFontPadding="false"
        android:singleLine="false"
        android:text="SIGNAL"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/signal"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/coords_title"
        app:layout_constraintVertical_bias="0.0" />
    <TextView
        android:id="@+id/mcc_label"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:includeFontPadding="false"
        android:singleLine="false"
        android:text="MCC"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/mcc"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/signal_label"
        app:layout_constraintVertical_bias="0.0" />

<!--
app:layout_constraintBottom_toTopOf="@+id/mnc_label"
        android:layout_marginTop="-20dp"
-->
    <TextView
        android:id="@+id/mnc_label"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:text="MNC"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/mnc"
        app:layout_constraintHorizontal_bias="0.0"
        android:includeFontPadding="false"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mcc_label" />
<!--    app:layout_constraintBottom_toTopOf="@+id/lac_label"-->
    <TextView
        android:id="@+id/lac_label"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:text="LAC"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/lac"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        android:includeFontPadding="false"
        app:layout_constraintTop_toBottomOf="@+id/mnc_label" />
<!--    app:layout_constraintBottom_toTopOf="@+id/cid_label"-->
    <TextView
        android:id="@+id/cid_label"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:text="CID"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/cid"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        android:includeFontPadding="false"
        app:layout_constraintTop_toBottomOf="@+id/lac_label" />
<!--    app:layout_constraintBottom_toTopOf="@+id/lat_label"-->
    <TextView
        android:id="@+id/enb_label"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:text="PCI"
        android:textStyle="bold"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/enb"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        android:includeFontPadding="false"
        app:layout_constraintTop_toBottomOf="@+id/cid_label" />
    <TextView
        android:id="@+id/lat_label"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:text="Latitude"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/lat"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        android:includeFontPadding="false"
        app:layout_constraintTop_toBottomOf="@+id/enb_label" />
<!--    app:layout_constraintBottom_toTopOf="@+id/lng_label"-->
    <TextView
        android:id="@+id/lng_label"
        android:layout_width="200dp"
        android:layout_height="40dp"
